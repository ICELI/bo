{
  "name": "bo",
  "version": "0.1.0",
  "description": "vue wheels",
  "main": "dist/bo.js",
  "scripts": {
    "dev": "rollup -w -c build/build.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commitmsg": "validate-commit-msg"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    }
  },
  "repository": "git+https://github.com/ICELI/bo.git",
  "author": "ICELI <woticeli@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "conventional-changelog": "^1.1.3",
    "conventional-changelog-cli": "^1.3.1",
    "flow-bin": "^0.46.0",
    "husky": "^0.13.3",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-watch": "^3.2.2",
    "validate-commit-msg": "^2.12.1"
  },
  "bugs": {
    "url": "https://github.com/ICELI/bo/issues"
  },
  "homepage": "https://github.com/ICELI/bo#readme",
  "keywords": [
    "bo.js",
    "vue.js",
    "vue-wheels"
  ]
}
